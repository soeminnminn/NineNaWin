<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GoogleProgressBar">
        <attr name="type" format="enum">
            <enum name="folding_circles" value="0" />
            <enum name="google_music_dices" value="1" />
            <enum name="nexus_rotation_cross" value="2" />
        </attr>
        <attr name="colors" format="reference"/>
    </declare-styleable>
    
    <color name="gpb_red">#FFC93437</color>
    <color name="gpb_blue">#FF375BF1</color>
    <color name="gpb_yellow">#FFF7D23E</color>
    <color name="gpb_green">#FF34A350</color>
    
    <integer-array name="gpb_google_colors">
        <item>@color/gpb_red</item>
        <item>@color/gpb_blue</item>
        <item>@color/gpb_yellow</item>
        <item>@color/gpb_green</item>
    </integer-array>
    <integer name="gpb_default_type">0</integer>
</resources>